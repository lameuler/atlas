---
import { Named } from '../model'
import Heading from './Heading.astro'
import DeclarationSection from './DeclarationSection.astro'

interface Props {
    named?: Named
}
const { named } = Astro.props
---

{
    named && (
        <>
            <Heading {...named.heading} />
            {named.declarations.map((declaration) => (
                <DeclarationSection {declaration} />
            ))}
        </>
    )
}
